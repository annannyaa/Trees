Print all nodes at depth 'd'/level 'd'
*****************************************************
void printatLevelK(TreeNode <int>*, int k){
if(root==NULL){          //edge case
return;
}
  if(k==0){
  cout<<root->data<<endl;
  return;
  }
  for(int i=0;i<root->children.size();i++){
  printatLevelK(root->children[i],k-1);
  }
}
